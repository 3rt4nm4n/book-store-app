<Window x:Class="LabWorkQ2.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWorkQ2"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="400">
    <Grid>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="92,165,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="95,209,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="usrnameTxtBox" HorizontalAlignment="Left" Height="23" Margin="188,169,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" Margin="188,264,0,0" VerticalAlignment="Top" Width="75" Click="loginButton_Click"/>
        <PasswordBox x:Name="passTxtBox" HorizontalAlignment="Left" Margin="188,209,0,0" VerticalAlignment="Top" Width="120" Height="26"/>

    </Grid>
</Window>
